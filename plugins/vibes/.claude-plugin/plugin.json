{
  "name": "claude-vibes",
  "version": "1.4.4",
  "description": "Production-grade vibe coding suite. Structured workflows from discovery to deployment: start, build, ship, fix, refactor.",
  "author": {
    "name": "Claude Vibes"
  },
  "mcpServers": {
    "context7": {
      "type": "http",
      "url": "https://mcp.context7.com/mcp"
    },
    "memory": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-memory"]
    },
    "sequential-thinking": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-sequential-thinking"]
    },
    "taskmaster": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "task-master-ai"],
      "env": {
        "TASK_MASTER_TOOLS": "standard"
      }
    },
    "whois": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "@mcp-server/whois-mcp@latest"]
    }
  }
}
